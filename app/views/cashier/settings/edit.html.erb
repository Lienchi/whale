<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h3><%= @setting.note %></h3>
      <%= form_for [:cashier, @setting] do |f| %>
        <div class="form-group">
         
          <%= f.label :content, "內容" %>
          <%= f.text_field :content, class: "form-control"%>
          <br>
          
          <%= f.submit "確定", class: "btn btn-primary" %>
          
        </div>
      <% end %>
   </div>
  </div>
</div>
